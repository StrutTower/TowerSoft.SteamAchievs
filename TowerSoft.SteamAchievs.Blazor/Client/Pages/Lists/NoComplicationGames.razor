@page "/List/NoComplicationGames"
@inject GameListDataService service

<h3>No Complication Games</h3>

@if (Model == null) {
    <Loading />
} else {
    <GameList Model="Model"></GameList>
}

@code {

    private GameListViewModel Model;

    protected async override Task OnInitializedAsync() {
        Model = await service.GetNoComplicationsGames();
    }
}
