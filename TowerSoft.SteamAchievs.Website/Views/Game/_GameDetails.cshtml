@model GameDetails

<div class="row mb-3">
    <div class="col-auto">
        <div class="column-label">Perfect Possible</div>
        <div display-for="PerfectPossible" template="CheckBoolean" class="text-center"></div>
    </div>
    <div class="col-auto">
        <div class="column-label">Play Next Score</div>
        <div display-for="PlayNextScore" class="text-center"></div>
    </div>
    <div class="col-auto">
        <div class="column-label">Finished</div>
        <div display-for="Finished" template="CheckBoolean" class="text-center"></div>
    </div>
    <div class="col-auto">
        <div class="column-label">ProtonDB Rating</div>
        <div display-for="ProtonDbRating" template="ProtonDbRating" class="text-center"></div>
    </div>
</div>